<body ng-app="mogiApp">
	@*<script type="text/javascript">@Html.Raw(mogijs)</script>*@
	@if (HappyRE.Core.Utils.Common.JavaScriptThirdparty)
	{
	<!-- Google Tag Manager -->
    <noscript><iframe src="//www.googletagmanager.com/ns.html?id=GTM-WC9T99"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
